<?xml version="1.0" encoding="utf-8"?>
<Project name="ATM_Altimeter">
    <Package name="Altimeter">
        <Key ID="STR_Altimeter_Name">
            <English>Altimeter</English>
            <French>Altimètre</French>
            <German>Höhenmesser</German>
        </Key>
        <Key ID="STR_Altimeter_Description">
            <English>The Military altimeter give you a bearing, The ASL(Altitude Sea Level), MapGrid, Stopwatch and the Hour.</English>
            <French>L'altimètre militaire vous donne un azimut, votre altitude par rapport au niveau de la mer, votre position géographique, un chronomètre et l'heure.</French>
            <German>Das militärische Höhenmessgerät gibt die Höhe über dem Meeresspiegel, die Blickrichtung und die Uhrzeit.</German>
        </Key>
        <Key ID="STR_Altimeter_KeyDisplay">
            <English>Show/Hide Altimeter</English>
            <French>Afficher/Cacher Altimètre</French>
            <German>Anzeigen/Verstecken des Höhenmessers</German>
        </Key>
        <Key ID="STR_Altimeter_KeyMove">
            <English>Move Altimeter</English>
            <French>Déplacer l'altimètre</French>
            <German>Bewege den Höhenmesser</German>
        </Key>
		<Key ID="STR_Altimeter_SelectMod">
            <English>Change Display</English>
            <French>Changer Affichage</French>
	    <German>Anzeige wechseln</German>
        </Key>
		<Key ID="STR_Altimeter_StartStopTime">
            <English>Start/Pause StopWatch</English>
            <French>Démarrer/Pause Chronomètre</French>
	    <German>Starten/Pausieren der Stopuhr</German>	
        </Key>
		<Key ID="STR_Altimeter_ResetTime">
            <English>Reset StopWatch</English>
            <French>Remettre à zéro le Chronomètre</French>
	    <German>Stopuhr zurücksetzen</German>
        </Key>
        <Key ID="STR_Altimeter_MoveText">
            <English>Move Me. Press Esc to cancel.</English>
            <French>Me déplacer. Appuyer sur Esc pour annuler.</French>
            <German>Bewege mich. Drücke Esc um Abzubrechen.</German>
        </Key>
        <Key ID="STR_Altimeter_MoveButtonText">
            <English>Validate the position.</English>
            <French>Valider la position.</French>
            <German>Bestätige die Position.</German>
        </Key>
        <Key ID="STR_Altimeter_InvalidPos">
            <English>Invalid position.</English>
            <French>Position non valide.</French>
            <German>Nicht valide Position.</German>
        </Key>
    </Package>
</Project>
